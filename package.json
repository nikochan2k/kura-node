{
  "name": "kura-node",
  "version": "0.4.24",
  "description": "The FileSystem API abstraction library, node Plugin",
  "author": "Yoshihiro Seki",
  "license": "MIT",
  "homepage": "https://github.com/nikochan2k/kura-node",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "keywords": [
    "kura",
    "FileSystem",
    "storage",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nikochan2k/kura-node.git"
  },
  "scripts": {
    "clean": "rimraf ./lib",
    "compile": "tsc",
    "copy": "cpx \"src/**/*.ts\" lib",
    "build": "npm-run-all clean compile copy",
    "npm-version": "npm version patch",
    "npm-publish": "npm publish",
    "release": "npm-run-all clean compile npm-version npm-publish",
    "test": "jest --runInBand"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "<rootDir>/src/__tests__/*.spec.+(ts|tsx|js)"
    ],
    "preset": "ts-jest"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^16.10.5",
    "@types/request": "^2.48.7",
    "cpx2": "^4.2.0",
    "jest": "^26.6.3",
    "kura": "^0.4.69",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.3.5"
  },
  "optionalDependencies": {
    "kura": "^0.4.61"
  },
  "dependencies": {
    "request": "^2.88.2"
  }
}
